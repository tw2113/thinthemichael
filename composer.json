{
  "name": "tw2113/thinthemichael",
  "description": "My plugins",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/cmb2": "2.10.1",
    "wpackagist-plugin/custom-post-type-ui": "1.13.7",
    "wpackagist-plugin/disable-comments": "2.4.4",
    "wpackagist-plugin/instagram-feed": "6.1.5",
    "wpackagist-plugin/query-monitor": "3.13.0",
    "wpackagist-plugin/blogroll-block": "1.3.3",
    "wpackagist-plugin/classic-widgets": "0.3"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
