{
  "name": "tw2113/thinthemichael",
  "description": "My plugins",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/cmb2": "2.10.1",
    "wpackagist-plugin/custom-post-type-ui": "1.12.1",
    "wpackagist-plugin/disable-comments": "2.4.1",
    "wpackagist-plugin/instagram-feed": "6.0.6",
    "wpackagist-plugin/query-monitor": "3.9.0",
    "wpackagist-plugin/blogroll-block": "1.2.0"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
