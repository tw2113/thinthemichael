{
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/cmb2": "2.6.0",
    "wpackagist-plugin/custom-post-type-ui": "1.7.1",
    "wpackagist-plugin/disable-comments": "1.10.2",
    "wpackagist-plugin/instagram-feed": "2.1.4",
    "wpackagist-plugin/query-monitor": "3.5.2"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  }
}
