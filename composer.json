{
  "name": "tw2113/thinthemichael",
  "description": "My plugins",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/blogroll-block": "1.3.5",
    "wpackagist-plugin/classic-widgets": "0.3",
    "wpackagist-plugin/cmb2": "2.11.0",
    "wpackagist-plugin/copyright-block": "0.1.1",
    "wpackagist-plugin/custom-post-type-ui": "1.15.1",
    "wpackagist-plugin/disable-comments": "2.4.6",
    "wpackagist-plugin/disable-emojis": "1.7.6",
    "wpackagist-plugin/instagram-feed": "6.2.10",
    "wpackagist-plugin/query-monitor": "3.15.0",
    "wpackagist-plugin/wp-migrate-db": "2.6.10"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    },
    "sort-packages": true
  }
}
