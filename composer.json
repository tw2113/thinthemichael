{
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/cmb2": "2.9.0",
    "wpackagist-plugin/custom-post-type-ui": "1.9.1",
    "wpackagist-plugin/disable-comments": "2.1.1",
    "wpackagist-plugin/instagram-feed": "2.9",
    "wpackagist-plugin/query-monitor": "3.6.7",
    "wpackagist-plugin/blogroll-block": "1.2.0"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  }
}
